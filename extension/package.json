{
  "name": "notebook-renderer-sample",
  "displayName": "notebook-renderer-sample",
  "description": "Example of a Notebook renderer (visualizer) for GitHub Issues Notebook that doesn't use any frontend frameworks.",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "dev": "rollup --watch --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "vscode:prepublish": "npm run build",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "extensionDependencies": [
    "vscode.sql",
    "vscode.python"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ducklab",
        "aliases": [
          "Ducklab"
        ],
        "extensions": [
          ".isql",
          ".sql"
        ],
        "firstLine": "# Ducklab"
      }
    ],
    "notebooks": [
      {
        "type": "ducklab",
        "displayName": "Ducklab",
        "selector": [
          {
            "filenamePattern": "*.isql"
          }
        ]
      }
    ],
    "commands": [
      {
        "title": "Toggle Debugging",
        "command": "ducklab.toggleDebugging",
        "icon": "$(bug)"
      },
      {
        "title": "Restart Kernel",
        "command": "ducklab.restartKernel",
        "icon": "$(refresh)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "ducklab.toggleDebugging",
          "when": "notebookEditorFocused",
          "group": "navigation@1"
        },
        {
          "command": "ducklab.restartKernel",
          "when": "notebookEditorFocused",
          "group": "navigation@2"
        }
      ]
    },
    "notebookRenderer": [
      {
        "id": "notebook-renderer-table",
        "entrypoint": "./out/renderer/table.js",
        "displayName": "Table",
        "mimeTypes": [
          "x-application/table"
        ]
      }
    ]
  },
  "devDependencies": {
    "@ducklab/core": "../core/ducklab-core-0.3.5.tgz",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/vscode": "^1.92.0",
    "@types/vscode-notebook-renderer": "^1.72.3",
    "apache-arrow": "^14.0.0",
    "duckdb-async": "^1.0.0",
    "rollup": "^4.20.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "^5.5.4",
    "vscode-notebook-error-overlay": "^1.0.1"
  }
}