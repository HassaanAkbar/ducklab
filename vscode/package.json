{
  "name": "notebook-renderer-sample",
  "displayName": "notebook-renderer-sample",
  "description": "Example of a Notebook renderer (visualizer) for GitHub Issues Notebook that doesn't use any frontend frameworks.",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "extensionDependencies": [
    "vscode.sql",
    "vscode.python"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ducklab",
        "aliases": [
          "Ducklab"
        ],
        "extensions": [
          ".isql",
          ".sql"
        ],
        "firstLine": "# Ducklab"
      }
    ],
    "notebooks": [
      {
        "type": "ducklab",
        "displayName": "Ducklab",
        "selector": [
          {
            "filenamePattern": "*.isql"
          }
        ]
      }
    ],
    "commands": [
      {
        "title": "Toggle Debugging",
        "command": "ducklab.toggleDebugging",
        "icon": "$(bug)"
      },
      {
        "title": "Restart Kernel",
        "command": "ducklab.restartKernel",
        "icon": "$(refresh)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "ducklab.toggleDebugging",
          "when": "notebookEditorFocused",
          "group": "navigation@1"
        },
        {
          "command": "ducklab.restartKernel",
          "when": "notebookEditorFocused",
          "group": "navigation@2"
        }
      ]
    },
    "notebookRenderer": [
      {
        "id": "notebook-renderer-table",
        "entrypoint": "./out/renderer/table.js",
        "displayName": "Table",
        "mimeTypes": [
          "x-application/table"
        ]
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/vscode-notebook-renderer": "^1.57.8",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^7.14.0",
    "@typescript-eslint/parser": "^7.14.0",
    "css-loader": "^4.2.0",
    "eslint": "^8.26.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^9.2.2",
    "typescript": "^5.5.2",
    "vscode-notebook-error-overlay": "^1.0.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@ducklab/core": "../core/ducklab-core-0.3.4.tgz",
    "apache-arrow": "^12.0.0",
    "duckdb-async": "^1.0.0"
  }
}